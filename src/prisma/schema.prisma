generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")

}

model Producto {
  id          Int      @id @default(autoincrement())
  nombre      String
  descripcion String?
  precio      Float
  stock       Int      @default(0)
  creadoEn    DateTime @default(now())
  actualizadoEn DateTime @updatedAt
}

model Purchase {
  id    Int      @id @default(autoincrement())
  date  DateTime @default(now())
  items Json
}
